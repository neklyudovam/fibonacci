#include <iostream>

int main() {
    std::cout << "Hello, World!" << std::endl;
    return 0;
}
void fib_iterative(int n) {
    int a = 0, b = 1;
    for (int i = 0; i < n; ++i) {
        std::cout << a << std::endl;
        int next = a + b;
        a = b;
        b = next;
    }
}

int main() {
    int n = 10;
    fib_iterative(n); // Выводит первые n чисел Фибоначчи
    return 0;
}
