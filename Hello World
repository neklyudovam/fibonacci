#include <iostream>

int main() {
    std::cout << "Hello, World!" << std::endl;
    return 0;
}
int fib(int n) {
    if (n <= 0) return 0;
    if (n == 1) return 1;
    return fib(n - 1) + fib(n - 2);
}

int main() {
    int n = 10;
    std::cout << fib(n) << std::endl; // Выводит 55 для fib(10)
    return 0;
}
